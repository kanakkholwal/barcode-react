{
    "name": "barcode-react",
    "version": "1.0.0",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup ./src/index.tsx --format cjs,esm --dts",
        "release": "pnpm run build && changeset publish",
        "lint": "tsc"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/",
        "tag": "latest",
        "directory": "dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kanakkholwal/pdf-tables-parser.git"
    },
    "bugs": {
        "url": "https://github.com/kanakkholwal/pdf-tables-parser/issues"
    },
    "author": "Kanak Kholwal (@kanakkholwal)",
    "license": "MIT",
    "keywords": [],
    "description": "A template repository for typescript packages",
    "dependencies": {
        "install": "^0.13.0",
        "jsbarcode": "^3.11.6",
        "react": "latest",
        "react-dom": "latest"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.12",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "tsup": "^8.3.5",
        "typescript": "^5.7.3"
    }
}